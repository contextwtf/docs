{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Context Docs",
  "colors": {
    "primary": "#090a0c",
    "light": "#fff",
    "dark": "#090a0c"
  },
  "background": {
    "color": {
      "dark": "#090a0c"
    }
